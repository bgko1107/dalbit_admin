<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dalbit.member.dao.M_MyStarDao" >

    <select id="getMyStarList" resultType="com.dalbit.member.vo.MyStarListVo" >
    select b.mem_id
    , b.mem_nick
    , f.reg_date
    from tb_member_fanstar f
    , tb_member_basic b
    where f.mem_no_star = b.mem_no
    </select>


</mapper>